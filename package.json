    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3",
    "ts-loader": "^4.2.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
